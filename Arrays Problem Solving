//       right--;
//     } else {
//       index++;
//     }
//   }




//   int zero = 0;
//   int one = 0;
//   int two = 0;

//   for (int i = 0; i < nums.size(); i++) {
//     if (nums[i] == 0) {
//       zero++;
//     } else if (nums[i] == 1) {
//       one++;
//     } else {
//       two++;
//     }
//   }

//   int index = 0;
//   for (int i = 0; i < zero; i++) {
//     nums[index++] = 0;
