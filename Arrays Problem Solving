#include <iostream>
#include <limits.h>
#include <string>
#include <vector>
using namespace std;

void sortColour(vector<int> &nums) {

     int n = nums.size();
  int left = 0, right = n - 1;
//   int index = 0;
 for (int index = 0; index <= right;) {
    if (nums[index] == 0) {
      swap(nums[index], nums[left]);
      index++;
      left++;

    } else if (nums[index] == 2) {
      swap(nums[index], nums[right]);
      right--;
    } else {
      index++;
    }
  }
}
//   while (index <= right) {
